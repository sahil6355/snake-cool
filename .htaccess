<IfModule mod_headers.c>
  <FilesMatch "\.unityweb$">
    Header set Content-Encoding gzip
    Header set Content-Type application/octet-stream
  </FilesMatch>
</IfModule>

<IfModule mod_mime.c>
  AddType application/wasm .wasm
</IfModule>

<IfModule mod_deflate.c>
  SetEnvIfNoCase Request_URI \.unityweb$ no-gzip
</IfModule>
